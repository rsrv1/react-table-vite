{
    "name": "react-table-vite",
    "private": true,
    "version": "0.0.1",
    "author": "Sourav Rakshit <hello@itsrav.dev> (https://twitter.com/srvrksh)",
    "type": "module",
    "scripts": {
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
        "build:tailwind": "npx tailwindcss -i ./src/styles/style.css -o ./dist/style.css -m",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
        "lint:fix": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
        "clean": "rm -rf node_modules && rm -rf dist && rm pnpm-lock.yaml"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@vitejs/plugin-react": "^3.0.0",
        "typescript": "^4.9.4",
        "prettier": "latest",
        "prettier-plugin-tailwindcss": "latest",
        "prettier-plugin-organize-imports": "latest",
        "eslint": "latest",
        "eslint-config-prettier": "latest",
        "eslint-plugin-react": "latest",
        "eslint-plugin-react-hooks": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "autoprefixer": "^10.4.13",
        "postcss": "^8.4.20",
        "tailwindcss": "^3.2.4",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.5.0",
        "@tailwindcss/line-clamp": "^0.4.0",
        "@tailwindcss/typography": "^0.5.0",
        "vite": "^4.0.0"
    }
}
